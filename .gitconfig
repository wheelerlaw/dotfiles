[push]
	default = current
[core]
	excludesfile = ~/.config/gitignore
[init]
	defaultBranch = main
[credential "https://github.com"]
	helper = 
	helper = !/usr/bin/op plugin run gh -- auth git-credential
[credential "https://gist.github.com"]
	helper = 
	helper = !/usr/bin/op plugin run gh -- auth git-credential
[include]
    path = ~/.local/etc/gitconfig

